AzQtComponents--TabWidget > AzQtComponents--TabWidgetActionToolBarContainer
{
    background: #111111;
    min-height: 28px;
    margin: 0;
    margin-bottom: 3px;
}

AzQtComponents--TabWidget.Secondary > AzQtComponents--TabWidgetActionToolBarContainer
{
    background: transparent;
}

AzQtComponents--TabWidgetActionToolBar QPushButton,
AzQtComponents--TabWidgetActionToolBar QPushButton:hover,
AzQtComponents--TabWidgetActionToolBar QPushButton:pressed
{
    qproperty-flat: true;
    background: transparent;
}